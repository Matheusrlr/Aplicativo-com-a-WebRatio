<Element name="Page" lowerName="page" pluralLowerName="pages">
	<Description>
		A page
		is a container of components, other sub pages, and alternative
		pages.
	</Description>
	<ActionRef label="Add Page" />
	<ActionRef label="Add Alternative" />
	<ActionRef label="Add Navigation Flow" />
	<ActionRef label="Add Activation Expression"/>
